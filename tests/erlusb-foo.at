AT_SETUP([erlusb-foo])
AT_KEYWORDS([erlusb foo])
AT_DATA([erlusb-foo.cmds], [dnl
erlusb:start("valgrind -v --show-reachable=yes --log-file=erlusb.vlog $AUTOTEST_PATH/erlusb").
erlusb:xxxyyy().
erlusb:xxx().
erlusb:aaa().
erlusb:stop().
init:stop().
])
exec 3> /tmp/erlusb-foo.log
echo "========================================" >&3
AT_CHECK([echo "PWD=$PWD" >&3], [0], [ignore])
AT_CHECK([echo "ERL=$ERL" >&3], [0], [ignore])
AT_CHECK([printenv >&3], [0], [ignore])
echo "----------------------------------------" >&3
dnl AT_CHECK([${ERL} -pa ${AUTOTEST_PATH}/src < erlusb-foo.cmds])
AT_CLEANUP
